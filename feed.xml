---
layout: null
permalink: /feed.xml
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ site.title | xml_escape }}</title>
    <description>{{ site.description | xml_escape }}</description>
    <link>{{ site.url }}{{ site.baseurl }}/</link>
    <atom:link href="{{ '/feed.xml' | prepend: site.baseurl | prepend: site.url }}" rel="self" type="application/rss+xml"/>
    <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    <generator>Jekyll v{{ jekyll.version }}</generator>
    {% for somaire in site.pages limit:10 %} {% if somaire.layout == 'sommaire' %}
      <item>
        <title>{{ somaire.title | xml_escape }}</title>
        <description>{{ somaire.content | xml_escape }}</description>
        <link>{{ somaire.url | prepend: site.baseurl | prepend: site.url }}</link>
        <guid isPermaLink="true">{{ somaire.url | prepend: site.baseurl | prepend: site.url }}</guid>
        {% for training in site.pages %} {% if training.categories == somaire.node %} {% if training.title %}
          <category>{{ training.title | xml_escape }}</category>
        {% endif %} {% endif %} {% endfor %}
      </item>
    {% endif %} {% endfor %}
  </channel>
</rss>
